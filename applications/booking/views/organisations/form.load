{{=required_message()}}
{{=form.custom.begin}}

<table class="form-table">
    <thead>
        <tr>
            <th class="form-label-col"></th>
            <th class="form-input-col"></th>
            <th class="form-label-col"></th>
            <th class="form-input-col"></th>
        </tr>
    </thead>
    <tbody>
        <tr class="shform">
            <td colspan="100%">Organisation Details<br/><hr/></td>
        </tr>
        
        <tr>
            <td><span class="m">*</span> {{=form.custom.label.name}}:</td>
            <td>{{=form.custom.widget.name}}</td>
           
           
        </tr>
        <tr>
            <td>{{=form.custom.label.desc}}:</td>
            <td>{{=form.custom.widget.desc}}</td>
        </tr>
    <tr>
            <td><span class="m">*</span> {{=form.custom.label.street_addr}}:</td>
            <td>{{=form.custom.widget.street_addr}}</td>
            <td><span class="m">*</span> {{=form.custom.label.suburb_addr}}:</td>
            <td>{{=form.custom.widget.suburb_addr}}</td>
        </tr>
        <tr>
            <td>{{=form.custom.label.city_addr}}:</td>
            <td>{{=form.custom.widget.city_addr}}</td>
            <td>{{=form.custom.label.postcode_addr}}:</td>
            <td>{{=form.custom.widget.postcode_addr}}</td>
        </tr>
                
        <tr class="shform">
            <td colspan="100%">Contact Details<br/><hr/></td>
        </tr>
        
        <tr>
            <td>Contact Name:</td>
            <td>{{=form.custom.widget.contact_name}}</td>
            <td>Contact Number:</td>
            <td>{{=form.custom.widget.contact_phone}}</td>
        </tr>
        <tr>
            <td>Contact Email:</td>
            <td>{{=form.custom.widget.contact_email}}</td>
            <td>Alternate Contact Number:</td>
            <td>{{=form.custom.widget.contact_mobile}}</td>
        </tr>
        <tr>
            <td>Comments:</td>
            <td>{{=form.custom.widget.comments}}</td>
        </tr>
        <tr class="shform">
            <td colspan="100%">Organisation Type<br/><hr/></td>
        </tr>
        <tr>
            <td><span class="m">*</span> {{=form.custom.label.organisations_types}}:</td>
            <td> <div id="organisation_type">{{=form.custom.widget.organisations_types}}</div></td>
        </tr>
  </tbody>
</table>
<tr></tr>
<div id="referral_organisation" hidden="hidden">
<table class="form-table">
    <tbody>    
        <tr>
            <td>{{=form.custom.label.is_disability_ref}}</td>
            <td>{{=form.custom.widget.is_disability_ref}}</td>
         </tr>
