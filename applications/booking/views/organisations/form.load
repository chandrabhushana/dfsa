    <tbody>                 
        <tr>
            <td>{{=form.custom.label.is_financial_sponsor}}</td>
            <td>{{=form.custom.widget.is_financial_sponsor}}</td>
        </tr>
        <tr>
            <td>{{=form.custom.label.is_in_kind_doanations}}</td>
